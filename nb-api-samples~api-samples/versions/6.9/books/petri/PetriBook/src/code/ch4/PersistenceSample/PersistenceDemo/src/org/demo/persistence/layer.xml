            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="PersistenceDemoTopComponent.settings" url="PersistenceDemoTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="editor">
                <file name="PersistenceDemoTopComponent.wstcref" url="PersistenceDemoTopComponentWstcref.xml"/>
