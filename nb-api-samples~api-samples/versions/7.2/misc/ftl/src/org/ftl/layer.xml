<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Editors">
        <folder name="text">
            <folder name="x-ftl">
                <attr name="displayName" bundlevalue="org.ftl.Bundle#Editors/text/x-ftl"/>
                <folder name="CompletionProviders">
                    <file name="org-netbeans-modules-html-editor-completion-HtmlCompletionProvider.instance"/>
                    <file name="org-netbeans-modules-html-editor-completion-HtmlPaletteCompletionProvider.instance"/>
                    <file name="org-netbeans-modules-csl-editor-completion-GsfCompletionProvider.instance"/>
                </folder>
                <folder name="BracesMatchers">
                    <file name="org-ftl-lexer-SJBracesMatcherFactory.instance">
                        <attr name="position" intvalue="0"/>
                    </file>
                </folder>
                <folder name="FontsColors">
                    <folder name="NetBeans">
                        <folder name="Defaults">
                            <file name="org-netbeans-modules-editor-ftl-token-colorings1.xml" url="FontAndColors1.xml"/>
                        </folder>
                    </folder>
                    <folder name="CityLights">
                        <folder name="Defaults">
                            <file name="org-netbeans-modules-editor-ftl-token-colorings2.xml" url="FontAndColors2.xml"/>
                        </folder>
                    </folder>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="OptionsDialog">
        <folder name="PreviewExamples">
            <folder name="text">
                <file name="x-ftl" url="FtlTemplate.ftl"/>
            </folder>
        </folder>
    </folder>
</filesystem>